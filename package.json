{
  "name": "toilet-app-mobile",
  "version": "0.1.0",
  "description": "Mobile MVP for Toilet App",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
